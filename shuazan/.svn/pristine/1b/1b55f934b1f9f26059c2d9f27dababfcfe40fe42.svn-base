*{padding: 0;margin: 0;text-decoration: none;}
html,body{ overflow-x:hidden; width: 100%;height:100%}
ul li{ list-style:none;}
a{color: #4a4a4a}
body{
    font-family: -apple-system, BlinkMacSystemFont, "PingFang SC","Helvetica Neue",STHeiti,"Microsoft Yahei",Tahoma,Simsun,sans-serif;
}
@media screen and (min-width: 320px) {html {font-size: 12px;}}

@media screen and (min-width: 360px) {html {font-size: 14px;}}

@media screen and (min-width: 400px) {html {font-size: 16px;}}

@media screen and (min-width: 440px) {html {font-size: 18px;}}

@media screen and (min-width: 480px) {html {font-size: 20px;}}

@media screen and (min-width: 640px) {html {font-size: 22px;}}
.clearfix:after{
    content: "";
    display: block;
    clear: both;
}
.line-gary{
    height:5px;
    background: #F4F4F4;
}
.fs30{font-size: 1.1rem;}
.fs24{font-size:1rem;}
.fs20{font-size: 1rem;}

.header{
    position: relative;
    padding:13px 0 19px 0;
    width: 100%;
    height:50px;
    text-align: center;
    box-sizing: border-box;
    border-bottom: 1px solid #e1e1e1;
    background: #FFFFFF;
}
.header .title{
    font-size:18px;
    color:#000000;
    font-weight: 400;
}
.header .back{
    position: absolute;
    top: 0;
    left: 0;
    width: 50px;
    height:100%;
    line-height: 50px;
}
.header .back img{
    width: 10px;
    height:19px;
    vertical-align: middle;
}
.tips{
    margin: 10px auto 0 auto;
    width: 80%;
    color: #9b9b9b;
    text-align: center;
}
.alert-tips{
    position: fixed;
    left: 50%;
    top:0;
    margin-left: -25%;
    padding: 10px 0;
    width: 50%;
    border-radius: 5px;
    background: #1DD5FF;
    text-align: center;
    color: #FFFFFF;
    font-size: 1rem;
    transition: all 0.3s ease;
}
.fade-enter-active, .fade-leave-active {
    transition: top .5s
}
.fade-enter{
    top: 0
}
.fade-leave-to{
    top: -40px
}
.warpper{
    position: relative;
    overflow: hidden;
}
