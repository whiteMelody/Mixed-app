body{
    background: #F4F4F4;
}
.input{
    padding:14px 15px;
    font-size: 0;
    background: #FFFFFF;
}
.input .warpper{
    position: relative;
}
.input input{
    width: 73%;
    height: 52px;
    border: 1px solid #979797;
    outline: none;
    border-right: none;
    text-align: center;
    font-size: 16px;
    color: #000000;
    border-radius: 5px 0 0 5px;
    box-sizing: border-box;
}
.input input::-webkit-input-placeholder{
    color: #9b9b9b;
    font-size: 1.2rem;
}
.input span{
    position: absolute;
    right: 0;
    top: 0;
    width: 27%;
    height: 52px;
    background: #25D3FF;
    font-size: 1.2rem;
    color: #FFFFFF;
    text-align: center;
    line-height: 52px;
    border-radius: 0 5px 5px 0;
}
.danger{
    padding:0 15px;
}
.danger .content{
    padding: 5px 0;
    width: 100%;
    font-size:1rem;
    color: #e8313d;
    letter-spacing:1px;
}
.list ul li{
    margin-top: 5px;
    padding: 8px 15px;
    background: #FFFFFF;
    line-height: 1.8rem;
    font-size: 1rem;
    color: #000000;
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2;
}
.list .loading{
    position: absolute;
    top: 55%;
    left: 50%;
    margin-left: -25px;
    margin-top: -5px;
    width: 50px;
    height: 10px;
}
.list .loading img{
    width: 100%;
    height:100%;
}